{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "api#start:dev": {
      "persistent": true,
      "cache": false
    },
    "client#dev": {
      "persistent": true,
      "cache": false
    }
  }
}
